// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance1101
func @performance1101() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4)[] : (d4 + -3 = 0 and -d0 + d3  = 0 and d2 + -7999 = 0 and d1  = 0 and d0 + -3999 >= 0 and -d0 + 7998 >= 0  or d4 + -2 = 0 and d0 + d3 + -7998 = 0 and d2 + -7999 = 0 and d1  = 0 and d0  >= 0 and -d0 + 3998 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<5,0>
  %e = presburger.equal %set, %r : !presburger.set<5,0>, !presburger.set<5,0>
  return %e : i1
}

