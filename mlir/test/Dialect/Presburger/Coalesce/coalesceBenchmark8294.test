// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8294
func @performance8294() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5)[p0, p1, p2] : (-d2 + d5  = 0 and -d1 + d4  + -1 = 0 and -d0 + d3  = 0 and d0  >= 0 and p0 -d0  + -1 >= 0 and d1  >= 0 and p1 -d1  + -2 >= 0 and d2  >= 0 and p2 -d2  + -1 >= 0  or -d2 + d5  = 0 and d4  = 0 and -d0 + d3  + -1 = 0 and -p1 d1  + 1 = 0 and p + -1 >= 0 and d0  >= 0 and p0 -d0  + -2 >= 0 and d2  >= 0 and p2 -d2  + -1 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<6,3>
  %e = presburger.equal %set, %r : !presburger.set<6,3>, !presburger.set<6,3>
  return %e : i1
}

