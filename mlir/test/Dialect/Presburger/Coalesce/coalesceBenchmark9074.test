// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance9074
func @performance9074() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"()[p0] : (-p >= 0  or p + -1 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<0,1>
  %e = presburger.equal %set, %r : !presburger.set<0,1>, !presburger.set<0,1>
  return %e : i1
}

