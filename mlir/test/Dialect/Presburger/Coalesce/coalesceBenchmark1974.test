// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance1974
func @performance1974() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5, d6)[] : (d2  = 0 and -d0  + 500 >= 0 and d1  + -1 >= 0 and -d1  + 998 >= 0 and d3  + -1 >= 0 and d0 + -d3  + -1 >= 0 and d4  + -1 >= 0 and -d4  + 998 >= 0 and d5  + -1 >= 0 and -d5  + 998 >= 0 and d6  >= 0 and -d6  + 3 >= 0  or -d0 + d3  = 0 and d2  = 0 and d0  + -1 >= 0 and -d0  + 500 >= 0 and d1  + -1 >= 0 and -d1  + 998 >= 0 and d4  + -1 >= 0 and -d4  + 998 >= 0 and d5  + -1 >= 0 and -d5  + 998 >= 0 and d6  >= 0 and -d6  + 3 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<7,0>
  %e = presburger.equal %set, %r : !presburger.set<7,0>, !presburger.set<7,0>
  return %e : i1
}

