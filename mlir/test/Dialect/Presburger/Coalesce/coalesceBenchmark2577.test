// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance2577
func @performance2577() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3)[] : (d3  = 0 and d2  = 0 and d1  = 0 and d0  + -1993 >= 0 and -d0  + 1998 >= 0  or d3  = 0 and d2  = 0 and d1  = 0 and d0  + -17 >= 0 and -d0  + 1991 >= 0  or d3  = 0 and d2  = 0 and d1  = 0 and d0  + -1992 = 0 )">

  %r = presburger.coalesce %set : !presburger.set<4,0>
  %e = presburger.equal %set, %r : !presburger.set<4,0>, !presburger.set<4,0>
  return %e : i1
}

