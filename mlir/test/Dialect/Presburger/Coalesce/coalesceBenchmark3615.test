// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance3615
func @performance3615() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3)[] : (d3  = 0 and d2 + -1192 = 0 and d1 + -999 = 0 and d0 + -1 >= 0 and -d0 + 499 >= 0  or d3  = 0 and d2 + -1184 = 0 and d1 + -999 = 0 and d0 + -1 >= 0 and -d0 + 499 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<4,0>
  %e = presburger.equal %set, %r : !presburger.set<4,0>, !presburger.set<4,0>
  return %e : i1
}

