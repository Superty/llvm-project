// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance2015
func @performance2015() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5, d6)[] : (d6 + -1 = 0 and d0 + d5 + -1999 = 0 and d4 + -2000 = 0 and d3 + -1999 = 0 and d2  = 0 and d1 + -2000 = 0 and d0 + -1 >= 0 and -d0 + 1999 >= 0  or d6 + -1 = 0 and d5 + -1999 = 0 and d4 + -2000 = 0 and d3 + -1999 = 0 and d2  = 0 and d1 + -2000 = 0 and d0  = 0 )">

  %r = presburger.coalesce %set : !presburger.set<7,0>
  %e = presburger.equal %set, %r : !presburger.set<7,0>, !presburger.set<7,0>
  return %e : i1
}

