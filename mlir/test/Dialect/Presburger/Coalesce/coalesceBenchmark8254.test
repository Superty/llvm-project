// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8254
func @performance8254() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2)[p0, p1, p2] : (p + -1 >= 0 and d0  >= 0 and p0 -d0  + -1 >= 0 and d1  >= 0 and p1 -d1  + -1 >= 0 and p2 -d2  + -2 >= 0  or p + -1 >= 0 and d0  >= 0 and p0 -d0  + -1 >= 0 and d1  >= 0 and p1 -d1  + -1 >= 0 and -p2 d2  >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<3,3>
  %e = presburger.equal %set, %r : !presburger.set<3,3>, !presburger.set<3,3>
  return %e : i1
}

