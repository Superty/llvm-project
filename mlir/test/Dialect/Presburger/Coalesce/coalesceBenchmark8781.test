// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8781
func @performance8781() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4)[] : (-d1 + d4  + -1 = 0 and -d0 + d3  = 0 and d2  + -1997 = 0 and d0  >= 0 and -d0  + 499 >= 0 and d1  >= 0 and -d1  + 1996 >= 0  or d4  = 0 and -d0 + d3  + -1 = 0 and d2  + -1997 = 0 and d1  + -1997 = 0 and d0  >= 0 and -d0  + 498 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<5,0>
  %e = presburger.equal %set, %r : !presburger.set<5,0>, !presburger.set<5,0>
  return %e : i1
}

