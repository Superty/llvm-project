// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance3839
func @performance3839() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3)[] : (d3  = 0 and d1  + -15 = 0 and d0  + -1 >= 0 and -d0  + 999 >= 0 and d2  + -497 >= 0 and -d2  + 498 >= 0  or d3  = 0 and d1  + -14 = 0 and d0  + -1 >= 0 and -d0  + 999 >= 0 and d2  + -497 >= 0 and -d2  + 498 >= 0  or d3  = 0 and d1  = 0 and d0  + -1 >= 0 and -d0  + 999 >= 0 and d2  + -497 >= 0 and -d2  + 498 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<4,0>
  %e = presburger.equal %set, %r : !presburger.set<4,0>, !presburger.set<4,0>
  return %e : i1
}

