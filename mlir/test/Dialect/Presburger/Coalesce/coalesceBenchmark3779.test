// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance3779
func @performance3779() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2)[] : (d2  + -2 = 0 and d0  + -1993 >= 0 and -d0  + 1999 >= 0 and d1  + -1 >= 0 and -d1  + 1998 >= 0  or d2  + -2 = 0 and d1  + -1999 = 0 and d0  + -1993 >= 0 and -d0  + 1999 >= 0  or d2  + -2 = 0 and d1  = 0 and d0  + -1993 >= 0 and -d0  + 1999 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<3,0>
  %e = presburger.equal %set, %r : !presburger.set<3,0>, !presburger.set<3,0>
  return %e : i1
}

