// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance2680
func @performance2680() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3)[] : (d3  + -2 = 0 and -d0 + d1  = 0 and d0  >= 0 and -d0 + d2  + -17 >= 0 and -d2  + 2799 >= 0  or d3  + -2 = 0 and -d0 + d1  = 0 and -d0  + 2799 >= 0 and d2  >= 0 and d0 + -d2  + -16 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<4,0>
  %e = presburger.equal %set, %r : !presburger.set<4,0>, !presburger.set<4,0>
  return %e : i1
}

