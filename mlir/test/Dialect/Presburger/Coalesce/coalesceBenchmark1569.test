// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance1569
func @performance1569() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3)[] : (d3  + -2 = 0 and d1  + -1096 = 0 and d0  = 0 and d2  + -897 >= 0 and -d2  + 898 >= 0  or d3  + -2 = 0 and d1  + -1096 = 0 and d0  = 0 and d2  + -1 >= 0 and -d2  + 896 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<4,0>
  %e = presburger.equal %set, %r : !presburger.set<4,0>, !presburger.set<4,0>
  return %e : i1
}

