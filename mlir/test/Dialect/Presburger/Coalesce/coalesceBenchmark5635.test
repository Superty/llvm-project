// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance5635
func @performance5635() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1)[p0] : (p0  + 1 >= 0 and -p0  + 1198 >= 0 and d1  + -999 >= 0  or p0  + 1 >= 0 and -p0  + 1198 >= 0 and -d1  + 997 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<2,1>
  %e = presburger.equal %set, %r : !presburger.set<2,1>, !presburger.set<2,1>
  return %e : i1
}

