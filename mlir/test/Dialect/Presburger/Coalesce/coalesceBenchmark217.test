// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance217
func @performance217() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3)[] : (d2 + 2147483647 >= 0 and -d2 + 2147483648 >= 0 and d2 + -d3 + -1 >= 0  or d2 + 2147483647 >= 0 and -d2 + 2147483648 >= 0 and -d2 + d3  >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<4,0>
  %e = presburger.equal %set, %r : !presburger.set<4,0>, !presburger.set<4,0>
  return %e : i1
}

