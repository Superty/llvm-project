// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance9099
func @performance9099() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1)[] : (d0 + -2 >= 0 and -d0 + 1024 >= 0  or d0  >= 0 and -d0 + 1022 >= 0  or d0 + -1 >= 0 and -d0 + 1023 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<2,0>
  %e = presburger.equal %set, %r : !presburger.set<2,0>, !presburger.set<2,0>
  return %e : i1
}

