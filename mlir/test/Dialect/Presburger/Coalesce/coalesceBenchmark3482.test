// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance3482
func @performance3482() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3)[] : (d2  + -3 = 0 and d0  >= 0 and -d0  + 7999 >= 0 and d1  >= 0 and -d1  + 7999 >= 0 and -d0 + 8d3  + 7 >= 0 and d0 + -8d3  >= 0  or d2  + -1 = 0 and d0  >= 0 and -d0  + 7999 >= 0 and d1  >= 0 and -d1  + 7999 >= 0 and -d0 + 8d3  + 7 >= 0 and d0 + -8d3  >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<4,0>
  %e = presburger.equal %set, %r : !presburger.set<4,0>, !presburger.set<4,0>
  return %e : i1
}

