// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance8243
func @performance8243() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3)[p0, p1, p2] : (p0 -1 >= 0 and p1 -1 >= 0 and p2 -1 >= 0 and p2 -d3 + -2 >= 0  or p0 -1 >= 0 and p1 -1 >= 0 and p2 -1 >= 0 and -p2 d3  >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<4,3>
  %e = presburger.equal %set, %r : !presburger.set<4,3>, !presburger.set<4,3>
  return %e : i1
}

