// RUN: mlir-opt -canonicalize %s | FileCheck %s

// CHECK-LABEL: func @performance4524
func @performance4524() -> i1 {
  // CHECK-NEXT: %[[S:.*]] = constant true
  // CHECK-NEXT: return %[[S]]
  %set = presburger.set #presburger<"(d0, d1, d2, d3, d4, d5, d6, d7, d8, d9)[] : (-d1 + d9  = 0 and -d0 + d8  = 0 and d7  = 0 and d6  + -2 = 0 and -d1 + d5  = 0 and d4  + -1 = 0 and -d0 + d3  = 0 and d2  + -1 = 0 and d0  >= 0 and -d0  + 998 >= 0 and d1  >= 0 and -d1  + 1199 >= 0 )">

  %r = presburger.coalesce %set : !presburger.set<10,0>
  %e = presburger.equal %set, %r : !presburger.set<10,0>, !presburger.set<10,0>
  return %e : i1
}

